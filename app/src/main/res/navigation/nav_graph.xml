<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/welcomeFrag2">

    <fragment
        android:id="@+id/homeFrag"
        android:name="com.example.chargepoint.HomeFrag"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFrag_to_ratesFrag"
            app:destination="@id/ratesFrag" />
        <action
            android:id="@+id/action_homeFrag_to_mapFrag"
            app:destination="@id/mapFrag" />
    </fragment>
    <fragment
        android:id="@+id/signUpFrag"
        android:name="com.example.chargepoint.SignUpFrag"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" />
    <fragment
        android:id="@+id/welcomeFrag2"
        android:name="com.example.chargepoint.WelcomeFrag"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFrag2_to_logInFrag2"
            app:destination="@id/logInFrag2" />
        <action
            android:id="@+id/action_welcomeFrag2_to_signUpFrag"
            app:destination="@id/signUpFrag" />
    </fragment>
    <fragment
        android:id="@+id/logInFrag2"
        android:name="com.example.chargepoint.LogInFrag"
        android:label="fragment_log_in"
        tools:layout="@layout/fragment_log_in" >
        <action
            android:id="@+id/action_logInFrag2_to_homeFrag"
            app:destination="@id/homeFrag" />
    </fragment>
    <fragment
        android:id="@+id/conditionsFrag"
        android:name="com.example.chargepoint.ConditionsFrag"
        android:label="fragment_conditions"
        tools:layout="@layout/fragment_conditions" />
    <fragment
        android:id="@+id/ratesFrag"
        android:name="com.example.chargepoint.ratesFrag"
        android:label="fragment_rates"
        tools:layout="@layout/fragment_rates" >
        <action
            android:id="@+id/action_ratesFrag_to_homeFrag"
            app:destination="@id/homeFrag" />
    </fragment>
    <fragment
        android:id="@+id/aboutFrag"
        android:name="com.example.chargepoint.aboutFrag"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/receiptFrag"
        android:name="com.example.chargepoint.receiptFrag"
        android:label="fragment_receipt"
        tools:layout="@layout/fragment_receipt" />
    <fragment
        android:id="@+id/mapFrag"
        android:name="com.example.chargepoint.mapFrag"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
</navigation>